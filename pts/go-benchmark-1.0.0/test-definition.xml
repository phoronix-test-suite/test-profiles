<?xml version="1.0"?>
<!--Phoronix Test Suite v6.2.0m4-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>Go Benchmarks</Title>
    <Description>Benchmark for monitoring real time performance of the Go implementation for HTTP, JSON and garbage testing per iteration.</Description>
    <ResultScale>Nanoseconds</ResultScale>
    <Proportion>LIB</Proportion>
    <SubTitle>Total For Average Test Times</SubTitle>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.0.0</Version>
    <SupportedPlatforms>Linux, BSD</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Processor</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>golang</ExternalDependencies>
    <EnvironmentSize>0.5</EnvironmentSize>
    <ProjectURL>https://github.com/golang/benchmarks.git</ProjectURL>
    <InternalTags>Go</InternalTags>
    <Maintainer>Gabriela Cervantes, Athenas Jimenez</Maintainer>
  </TestProfile>
  <TestSettings>
    <Option>
      <DisplayName>Test</DisplayName>
      <Identifier>run-test</Identifier>
      <ArgumentPrefix></ArgumentPrefix>
      <ArgumentPostfix></ArgumentPostfix>
      <DefaultEntry>0</DefaultEntry>
      <Menu>
        <Entry>
          <Name>http</Name>
          <Value>-bench=http</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>json</Name>
          <Value>-bench=json</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>garbage</Name>
          <Value>-bench=garbage</Value>
          <Message></Message>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
